# OpenRouter API Key (required for LLM calls)
# Get your key at: https://openrouter.ai/keys (free tier available)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Jina AI API Key (required for web search and scraping)
# Get your key at: https://jina.ai/ (free tier available)
JINA_API_KEY=your_jina_api_key_here

# Database URL (SQLite - no setup needed)
DATABASE_URL="file:./dev.db"

# Supabase (optional - for cloud analytics)
# Get your keys at: https://supabase.com/dashboard/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
